{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "DatadogLogs",
        "Args": {
          "apiKey": "",
          "source": "csharp",
          "service": "haseroz.boilerplate.auth",
          "host": "GABRIEL",
          "tags": [ "env:development", "application:haseroz" ],
          "configuration": {
            "url": "https://http-intake.logs.us5.datadoghq.com"
          }
        }
      }
    ]
  }
}
